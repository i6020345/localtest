var movie_url = '';
function change_tap_slide(_no) {
	galleryThumbs = new Swiper('.gallery-thumbs'+_no, {
		spaceBetween: 10,
		slidesPerView: 3,
		centeredSlides: true,
		loop: true,
		freeMode: true,
		loopedSlides: 5, //looped slides should be the same
		watchSlidesVisibility: true,
		watchSlidesProgress: true
	});

	galleryTops = new Swiper('.gallery-top'+_no, {
		loop: true,
		centeredSlides: true,
		spaceBetween: 30,
		loopedSlides: 5, //looped slides should be the same
		thumbs: {
			swiper: galleryThumbs,
		},
		navigation: {
			nextEl: '.arrow.right_arrow_btn',
			prevEl: '.arrow.left_arrow_btn'
		},
		pagination: {
			el: '.swiper-pagination',
			type: 'bullets'
		}
	});

	galleryThumbs.on('slideChange', function () {
		galleryTops.activeIndex = galleryThumbs.activeIndex;
	});

	galleryTops.on('slideChange', function () {
		galleryThumbs.activeIndex = galleryTops.activeIndex;

		var r_idx = galleryTops.activeIndex;
		var slide = galleryTops.slides[r_idx];
		$('input[name=spread_check_img]').val($(slide).children('img').data('idx'));
		movie_url = $(slide).children('iframe').data('movie');

		if (movie_url != '' && movie_url != undefined) {
			var iframe = $(slide).children('iframe')[0].contentWindow;
			iframe.postMessage('{"event":"command","func":"pauseVideo","args":""}','*');
		}
	});
}
